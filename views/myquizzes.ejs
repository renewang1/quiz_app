<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Home Page</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Hanalei+Fill&family=Roboto:wght@400;700&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <link rel="stylesheet" href="/styles/navbar.css" type="text/css" />
    <link rel="stylesheet" href="/styles/myquizzes.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/quizzes.js"></script>
  </head>

  <body>
    <header>
      <%- include('navbar') %>
    </header>
      <main class="quiz-container">
        <div class="quizbox">
          <div class="withinthebox">
            <div class="upperquizbox">
              <div class="leftupper">
                <span class="titleofquiz">Title</span>
                <span class="descriptionofquiz">description</span>
              </div>
              <button class="sharequizbutton">Share Quiz</button>
            </div>
            <div class="middlequizbox">
              <span class="attemptby">Username</span>
              <span class="resultofattempt">4/10</span>
            </div>
            <div class="lowerquizbox">
              <button class="shareresultsbutton">Share Results</button>
            </div>
          </div>
        </div>
      </main>
  </body>
</html>
